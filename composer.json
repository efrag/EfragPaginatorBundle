{
    "name": "efrag/paginator-bundle",
    "description": "This bundle creates a paginator service for the Symfony framework",
    "keywords": [],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Eleni Fragkiadaki",
            "email": "eleni.fra@gmail.com"
        }
    ],
    "require": {
        "php": ">7.1.3",
        "symfony/symfony": "3.4.*",
        "symfony/framework-bundle": "3.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0||^8.0",
        "matthiasnoback/symfony-config-test": "4.*",
        "matthiasnoback/symfony-dependency-injection-test": "4.*"
    },
    "autoload": {
        "psr-0": { "Efrag\\Bundle\\PaginatorBundle": "" }
    },
    "target-dir": "Efrag/Bundle/PaginatorBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
